{
  "name": "eleventy-starter",
  "version": "1.0.0",
  "description": "Boilerplate for basic 11ty page",
  "main": ".eleventy.js",
  "scripts": {
    "start": "npx sass src/scss:src/_includes/css && concurrently 'npx sass src/scss:src/_includes/css --watch' 'npx eleventy --serve --incremental'",
    "production": "NODE_ENV=production npx sass src/scss:src/_includes/css && NODE_ENV=production npx eleventy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.8",
    "@cagov/11ty-build-system": "^1.0.1",
    "clean-css": "^5.3.3",
    "dotenv": "^16.3.1",
    "eleventy-plugin-svg-sprite": "^2.4.2",
    "esbuild": "^0.25.0",
    "esbuild-plugin-glob": "^2.2.3",
    "gorko": "^0.9.1",
    "gsap": "^3.12.5",
    "html-minifier": "^4.0.0",
    "jsdom": "^23.0.1",
    "markdown-it": "^12.3.2",
    "markdown-it-footnote": "^4.0.0",
    "purgecss": "^4.1.3",
    "sass": "^1.69.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "postcss-preset-env": "^9.3.0"
  }
}
