[project]
	description = A universal root. You mount it, and it's mostly Go source with the exception of 5 binaries. 
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/ron minnich (rminnich)
[access "refs/for/refs/*"]
	read = group user/ron minnich (rminnich)
	push = group user/ron minnich (rminnich)
	label-Code-Review = -2..+2 group user/ron minnich (rminnich)
	label-Verified = -1..+1 group user/ron minnich (rminnich)
	rebase = group user/ron minnich (rminnich)
	removeReviewer = group user/ron minnich (rminnich)
	submit = group user/ron minnich (rminnich)
[access "refs/heads/*"]
	read = group user/ron minnich (rminnich)
	create = group user/ron minnich (rminnich)
	push = +force group user/ron minnich (rminnich)
	pushMerge = group user/ron minnich (rminnich)
[access "refs/tags/*"]
	push = +force group user/ron minnich (rminnich)
	pushTag = group user/ron minnich (rminnich)
	pushSignedTag = group user/ron minnich (rminnich)
